<% if(success && success.length){ %>
  <div class="alert alert-success fade show text-center" role="alert" style="margin-top: 100px;">
    <%= success %>
  </div>
  <% } %>

    <% if(error && error.length){ %>
      <div class="alert alert-danger fade show text-center" role="alert" style="margin-top: 100px;">
        <%= error %>
      </div>
      <% } %>