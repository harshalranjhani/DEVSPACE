<% layout('layouts/boilerplate') %>
    <% if(err.statusCode!==404) { %>
        <div class="row mt-5">
            <div class="col-6 offset-3">
                <div class="alert alert-danger" role="alert">
                    <h4 class="alert-heading">
                        SOMETHING WENT WRONG
                    </h4>
                    <!-- <!-- <p>Looks like you encountered an Error!</p> -->
            </div>
        </div>
        <% } else { %>
            <h1 class="display-5" id="mobile-header">Page Not Found!</h1>
            <div class="row">
                <div class="col-6">
                    <div class="d-flex justify-content-md-center align-items-center">
                        <img id="notfound" src="imgs/undraw_page_not_found_re_e9o6.svg" class="mb-3"
                            alt="No Results Matched">
                    </div>
                </div>
                <div class="col-6">
                    <div id="nf-text" class="display-5 mt-5">
                        Page Not Found!
                    </div>
                </div>
            </div>
            <% } %>